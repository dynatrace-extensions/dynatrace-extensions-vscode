		"@types/node-forge": "^1.0.2",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"@vscode/test-electron": "^2.1.3",
		"esbuild": "^0.15.7",
		"eslint": "^8.14.0",
		"mocha": "^9.2.2",
		"mock-fs": "^5.1.4",
		"typescript": "^4.6.4"
	},
