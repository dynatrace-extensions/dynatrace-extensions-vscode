				{
					"command": "dt-ext-copilot-workspaces.addWorkspace",
					"when": "view == dt-ext-copilot-workspaces",
					"group": "navigation"
				},
				{
					"command": "dt-ext-copilot-workspaces.refresh",
					"when": "view == dt-ext-copilot-workspaces",
					"group": "navigation"
				},
				{
